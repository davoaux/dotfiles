{
  "clangd.enabled": true,
  "clangd.disableDiagnostics": false,
  "coc.preferences.formatOnSaveFiletypes": [
    "javascript",
    "typescript",
    "css",
    "html",
    "markdown"
  ],
  "diagnostic.errorSign": "✘",
  "diagnostic.warningSign": "",
